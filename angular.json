{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "todo-app": {
      "root": "apps/todo-app/",
      "sourceRoot": "apps/todo-app/src",
      "projectType": "application",
      "prefix": "angular-monorepo",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/todo-app",
            "index": "apps/todo-app/src/index.html",
            "main": "apps/todo-app/src/main.ts",
            "polyfills": "apps/todo-app/src/polyfills.ts",
            "tsConfig": "apps/todo-app/tsconfig.app.json",
            "assets": [
              "apps/todo-app/src/favicon.ico",
              "apps/todo-app/src/assets"
            ],
            "styles": ["apps/todo-app/src/styles.css"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/todo-app/src/environments/environment.ts",
                  "with": "apps/todo-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "todo-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "todo-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "todo-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "tsConfig": [
              "apps/todo-app/tsconfig.app.json",
              "apps/todo-app/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/todo-app/jest.config.js",
            "tsConfig": "apps/todo-app/tsconfig.spec.json",
            "setupFile": "apps/todo-app/src/test-setup.ts"
          }
        }
      }
    },
    "todo-app-e2e": {
      "root": "apps/todo-app-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/todo-app-e2e/cypress.json",
            "tsConfig": "apps/todo-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "todo-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "todo-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "tsConfig": "apps/todo-app-e2e/tsconfig.e2e.json",
            "config": "tslint.spec.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "bookmark-manager": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/bookmark-manager",
      "sourceRoot": "apps/bookmark-manager/src",
      "prefix": "angular-monorepo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/bookmark-manager",
            "index": "apps/bookmark-manager/src/index.html",
            "main": "apps/bookmark-manager/src/main.ts",
            "polyfills": "apps/bookmark-manager/src/polyfills.ts",
            "tsConfig": "apps/bookmark-manager/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/bookmark-manager/src/favicon.ico",
              "apps/bookmark-manager/src/assets"
            ],
            "styles": ["apps/bookmark-manager/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/bookmark-manager/src/environments/environment.ts",
                  "with": "apps/bookmark-manager/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "bookmark-manager:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "bookmark-manager:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bookmark-manager:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "tsConfig": [
              "apps/bookmark-manager/tsconfig.app.json",
              "apps/bookmark-manager/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/bookmark-manager/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/bookmark-manager/jest.config.js",
            "tsConfig": "apps/bookmark-manager/tsconfig.spec.json",
            "setupFile": "apps/bookmark-manager/src/test-setup.ts"
          }
        }
      }
    },
    "bookmark-manager-e2e": {
      "root": "apps/bookmark-manager-e2e",
      "sourceRoot": "apps/bookmark-manager-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/bookmark-manager-e2e/cypress.json",
            "tsConfig": "apps/bookmark-manager-e2e/tsconfig.e2e.json",
            "devServerTarget": "bookmark-manager:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "bookmark-manager:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "config": "tslint.spec.json",
            "tsConfig": ["apps/bookmark-manager-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/bookmark-manager-e2e/**"]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "todo-app"
}
